<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\TypeOCore\TypeOCore.csproj" />
    <ProjectReference Include="..\..\TypeODesktop\TypeODesktop.csproj" />
    <ProjectReference Include="..\..\TypeOSDL\TypeOSDL.csproj" />
  </ItemGroup>

    <Target Name="PostBuild" AfterTargets="PostBuildEvent">
        <Exec Command="
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\libfreetype-6.dll&quot; &quot;$(ProjectDir)$(OutDir)\libfreetype-6.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\libjpeg-9.dll&quot; &quot;$(ProjectDir)$(OutDir)\libjpeg-9.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\libpng16-16.dll&quot; &quot;$(ProjectDir)$(OutDir)\libpng16-16.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\libtiff-5.dll&quot; &quot;$(ProjectDir)$(OutDir)\libtiff-5.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\libwebp-7.dll&quot; &quot;$(ProjectDir)$(OutDir)\libwebp-7.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\LICENSE.freetype.txt&quot; &quot;$(ProjectDir)$(OutDir)\LICENSE.freetype.txt&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\LICENSE.jpeg.txt&quot; &quot;$(ProjectDir)$(OutDir)\LICENSE.jpeg.txt&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\LICENSE.png.txt&quot; &quot;$(ProjectDir)$(OutDir)\LICENSE.png.txt&quot;&#xD;&#xA;
