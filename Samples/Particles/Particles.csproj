          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\LICENSE.webp.txt&quot; &quot;$(ProjectDir)$(OutDir)\LICENSE.webp.txt&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\LICENSE.zlib.txt&quot; &quot;$(ProjectDir)$(OutDir)\LICENSE.zlib.txt&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\SDL2.dll&quot; &quot;$(ProjectDir)$(OutDir)\SDL2.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\SDL2_image.dll&quot; &quot;$(ProjectDir)$(OutDir)\SDL2_image.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\SDL2_ttf.dll&quot; &quot;$(ProjectDir)$(OutDir)\SDL2_ttf.dll&quot;&#xD;&#xA;
          cp &quot;$(ProjectDir)..\..\TypeOSDL\SDL2\zlib1.dll&quot; &quot;$(ProjectDir)$(OutDir)\zlib1.dll&quot;" />
